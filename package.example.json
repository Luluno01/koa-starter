    "@types/koa__router": "^12.0.0",
    "@types/koa-log4": "^2.3.3",
    "@types/node": "^18.11.9",
    "@types/validator": "^13.7.10",
    "koa": "^2.11.0",
    "koa-log4": "^2.3.2",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
